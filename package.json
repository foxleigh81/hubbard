{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"yarn server\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "server": "node server.js",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "run-s test:*",
    "test:unit": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --verbose --coverage",
    "test:ts": "tsc",
    "test:audit": "yarn audit --fix",
    "lint": "run-s lint:*",
    "lint:ts": "eslint-output \"./components\" \"./pages\" --ext .ts,.tsx --fix",
    "lint:styles": "stylelint  \"./components/**/*.scss\" \"./pages/**/*.scss\" --fix --output-file \"./reports/stylelint-report.json\" --formatter json",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write ."
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@graphile-contrib/pg-many-to-many": "^1.0.1",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@storybook/addon-knobs": "^6.2.2",
    "@storybook/addon-viewport": "^6.2.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.2",
    "@testing-library/jest-dom": "^5.11.10",
    "babel-preset-react-app": "^10.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "css-modules-typescript-loader": "^4.0.1",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.23.0",
    "eslint-output": "^2.0.3",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "file-loader": "^6.2.0",
    "full-icu": "^1.3.4",
    "graphql": "^15.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "markdown-loader": "^6.0.0",
    "next": "latest",
    "npm-run-all": "^4.1.5",
    "postgraphile": "^4.11.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "sonarjs": "^1.0.0",
    "storybook": "^6.2.2",
    "storybook-readme": "^5.0.9",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-prettier": "^1.2.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "typescript": "4.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie > 0",
    "not op_mini all"
  ],
  "license": "MIT"
}
